<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
  <union-subclass name="pelarsServer.Ide" extends="pelarsServer.BaseData" table="Ides">
    
         <property name="opt">
            <column name="OPT" not-null="true" />
        </property>
        
        <property name="action_id">
            <column name="ACTION_ID" not-null="true" />
        </property>
        
      </union-subclass>
</hibernate-mapping>