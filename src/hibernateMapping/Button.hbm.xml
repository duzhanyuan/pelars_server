<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
 
  <union-subclass name="pelarsServer.Button" extends="pelarsServer.BaseData" table="Buttons">
       
    	  <property name="name">
            <column name="NAME"  not-null="true" />
        </property>
 
        <property name="data" >
            <column name="DATA"  not-null="true" />
        </property> 
     
    </union-subclass>
</hibernate-mapping>